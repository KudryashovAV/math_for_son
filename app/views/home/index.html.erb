<div style="border-style: groove; justify-content: center;display: flex;background: red;color: blue;">
  <a href="#" onclick="PrintDiv();">print me</a>
</div>

<div id=divToPrint>
  <% examples.each_slice(40) do |col| %>
    <div style="width: 1200px;">
      <div style="width: 250px; height: 1650px; float: left;">
        <% col.each do |x| %>
          <ul>
            <li><%= x %></li>
          </ul>
        <% end %>
      </div>
    </div>
  <% end %>
</div>



<script type="text/javascript">
  function PrintDiv() {
    var divToPrint = document.getElementById('divToPrint');
    var popupWin = window.open('', '_blank');
    popupWin.document.open();
    popupWin.document.write('<html><body onload="window.print()">' + divToPrint.innerHTML + '</html>');
    popupWin.document.close();
  }
</script>
